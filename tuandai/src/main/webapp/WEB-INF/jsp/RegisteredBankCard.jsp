    <script>
        $(document).ready(function () {
        	
        	initBankInfo(3000000119, 'CARDBIN');
            
            BHAFrom.data.hasCardNo = true;
            
            if ($('#bankcardNo').val()) {
                BHAFrom.ready = function (form) {
                    $('#bankcardNo').nextAll('.inpPrompt').remove();
                    form.check(false, $('#bankcardNo'));
                };
            }
            BHAFrom.config.beforeSubmit = function (curform) {

                var isAgreeReg = $("#isAgreeReg").prop("checked");
                if(!isAgreeReg){  //未选中注册协定
                    $("#nextButton").parent().append('<div class="inpPrompt" ><i class="icon-error"></i>请阅读并勾选用户协议</div>');
                    return false;
                }
                if (BHAFrom.data.agreeUseCard || BHAFrom.data.swiftPay || BHAFrom.data.hasCardNo) {
                	loading();  
                    return true;
                } else {
                    // 需要新线程才生效,奇怪
                    setTimeout(function () {
                        showDialogBottom();
                        if (BHAFrom.data.hasCardNo) {
                            $("#old-card-tip").show();
                        } else {
                            $("#new-card-tip").show();
                        }
                    }, 10);
                    return false;
                }
            };

            $("#isAgreeReg").click(function () {

                 var isAgreeReg = $("#isAgreeReg").prop("checked");
                 $("#nextButton").parent().find(".inpPrompt").remove();
                 if(!isAgreeReg){
                     $("#nextButton").parent().append('<div class="inpPrompt" ><i class="icon-error"></i>请阅读并勾选用户协议</div>');
                 }else{
                     $("#nextButton").parent().append('');
                 }
            });

            $('.agree-use-card').click(function () {
                BHAFrom.data.agreeUseCard = true;
                BHAFrom.form.submitForm();
            })
            $('.change-card').click(function () {
                $('#bankcardNo').val('');
                $('.bank_area').hide();
                BHAFrom.form.submitForm();
                $("#alertLayer,.alertLayer").hide();
                $("#mask").hide();
            })
            $(".selectDiv").mouseover(function () {
                $(".selectUl").show();
                $(".selectDiv i").addClass("arrow-up");
            }).mouseout(function () {
                $(".selectUl").hide();
                $(".selectDiv i").removeClass("arrow-up");
            });
            smsForNewPhone.init($("#sendSmsVerify"), {
                requestKey: '45d0ec2c7ec64d2dbea479966a633e33',
                phoneDom:'#mobile',
                smsCount: '',
            });
            
            BHAFrom.before = function (curform) {
                $('#smsCount').val(timer.c == 60 ? 0 : timer.c);
            }
            $(".protocol").click(function () {
                showDialogBottom();
                $(".modelContend").hide();
                $(".modelContend." + $(this).data('type')).show();
                $("#alertLayer").show();
            });
            
            
            
            //证件类型下拉框监听事件
            $("#newCredType").change(function(){
            	//获取选中的值
            	var selectedValue = $("#newCredType").val();
            	if(selectedValue=="PRC_ID"){
            		document.getElementById("nochuan").style.display='inline';
            		document.getElementById("ji").style.display='inline';
            		document.getElementById("accountTip").style.display='inline';
            	}else{
            		document.getElementById("nochuan").style.display='none';
            		document.getElementById("ji").style.display='none';
            		document.getElementById("accountTip").style.display='none';
            	}
            });

        });
    </script>
